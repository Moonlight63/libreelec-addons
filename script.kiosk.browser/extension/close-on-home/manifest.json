{
    "name": "Force Close on Home",
    "version": "1.0",
    "description": "Close chrome on home button",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "js/contentScript.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "js": ["js/contentScript.js"]
        }
    ]
}